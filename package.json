{
  "name": "zacilenonzadravi.cz",
  "version": "1.0.0",
  "scripts": {
    "run": "parcel src/index.html",
    "build": "parcel build src/index.html --dist-dir build/default",
    "build-public": "parcel build src/index.html --public-url '/' --dist-dir build/public",
    "clean-build": "rm -rf build/default",
    "clean-build-public": "rm -rf build/public",
    "clean": "npm run clean-build & npm run clean-build-public"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "parcel": "^2.8.3",
    "postcss": "^8.4.21",
    "sass": "^1.57.1",
    "bootstrap": "^5.2.3"
  }
}
